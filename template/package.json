{
  "name": "typescript-react",
  "version": "0.0.6",
  "description": "typescript react starter",
  "homepage": "https://github.com/juliantellez/typescript-react#readme",
  "scripts": {
    "build": "npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack --env.NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "build:analyze": "npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack ---env.ANALYZE --env.NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "clean": "rm -rf dist **/*.scss.d.ts",
    "dev:server": "TS_NODE_PROJECT=tsconfig.json webpack-dev-server --env.NODE_ENV=development --config ./config/webpack/webpack.config.ts",
    "doc": "typedoc",
    "lint": "eslint --ext .tsx --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run types:sass:watch | npm run dev:server",
    "storybook:start": "npm run types:sass:watch | start-storybook -p 6006",
    "storybook:build": "npm run types:sass && build-storybook",
    "test": "jest",
    "test:watch": "jest --watch --no-coverage",
    "test:coverage": "jest --coverage",
    "types:sass": "npm run clean && tsm src --camelCase",
    "types:sass:watch": "npm run types:sass -- --watch"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@storybook/addon-actions": "5.3.9",
    "@storybook/addon-links": "5.3.9",
    "@storybook/addons": "5.3.9",
    "@storybook/react": "5.3.9",
    "@testing-library/react": "9.4.0",
    "@types/copy-webpack-plugin": "5.0.0",
    "@types/html-webpack-plugin": "3.2.2",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/webpack": "4.41.3",
    "@types/webpack-bundle-analyzer": "2.13.3",
    "@types/webpack-dev-server": "3.10.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^25.1.0",
    "jest-watch-typeahead": "^0.5.0",
    "node-sass": "4.13.1",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "ts-node": "8.6.2",
    "typed-scss-modules": "1.1.0",
    "typedoc": "0.16.9",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.2"
  },
  "dependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  }
}
