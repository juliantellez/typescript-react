{
  "scripts": {
    "build": "npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack --env NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "build:analyze": "npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack --env ANALYZE --env NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "clean": "rm -rf dist **/*.scss.d.ts",
    "dev:server": "TS_NODE_PROJECT=tsconfig.json webpack-dev-server --env NODE_ENV=development --config ./config/webpack/webpack.config.ts",
    "doc": "typedoc",
    "lint": "eslint --ext .tsx --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run types:sass:watch | npm run dev:server",
    "storybook:start": "npm run types:sass:watch | start-storybook -p 6006",
    "storybook:build": "npm run types:sass && build-storybook",
    "test": "jest",
    "test:watch": "jest --watch --no-coverage",
    "test:coverage": "jest --coverage",
    "types:sass": "npm run clean && typed-scss-modules src --camelCase",
    "types:sass:watch": "npm run types:sass -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@testing-library/react": "^13.4.0",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-watch-typeahead": "^2.2.0",
    "node-sass": "^7.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "typed-scss-modules": "^7.0.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
